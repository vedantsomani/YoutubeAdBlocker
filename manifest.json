{
    "manifest_version": 3,
    "name": "YouTube Ad Blocker",
    "version": "1.0",
    "description": "Blocks YouTube ads by skipping video ads and removing ad elements.",
    "permissions": ["scripting"],
    "host_permissions": ["*://www.youtube.com/*"],
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  